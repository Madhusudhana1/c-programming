#include <stdio.h>

// Function to find the cube of a number
int cube(int num) {
    return num * num * num;
}

int main() {
    int num;

    // Take input from user
    printf("Enter a number: ");
    scanf("%d", &num);

    // Find the cube of the given number using the cube function
    int result = cube(num);

    // Print the result
    printf("The cube of %d is %d", num, result);

    return 0;
}
